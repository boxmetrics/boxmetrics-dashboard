  node {
    stage('Checkout') {
       checkout scm
    }
    stage('Environment') {
      sh 'git --version'
      echo "Branch: ${env.BRANCH_NAME}"
      sh 'docker -v'
    }
    stage('Build Docker test'){
     sh 'docker build -t boxmetrics-front-app .'
    }
    stage('Deploy'){
        sh 'docker tag boxmetrics-front-app 149.91.90.168:5000/boxmetrics-front-app'
        sh 'docker push 149.91.90.168:5000/boxmetrics-front-app'
      }
    }
  
  