  node {
    stage('Checkout') {
       checkout scm
    }
    stage('Environment') {
      sh 'git --version'
      echo "Branch: ${env.BRANCH_NAME}"
      sh 'docker -v'
    }
    stage('Build Docker test'){
     sh 'docker build -t boxmetrics-front-app .'
    }
    stage('Deploy'){
      
        sh 'docker login -u maxence -p maxence registry.labo-devops.ml'
        sh 'docker tag boxmetrics-front-app registry.labo-devops.ml/boxmetrics-front-app'
        sh 'docker push registry.labo-devops.ml/boxmetrics-front-app'
          
        
      }
    }
  
  